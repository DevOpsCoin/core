# Postgres
POSTGRES_USER=devops
POSTGRES_PASSWORD=devops_password
POSTGRES_DB=devops_db

# Database URL used by the app (example)
DATABASE_URL=postgresql://devops:devops_password@db:5432/devops_db

# Next.js env
NEXT_PUBLIC_TRADING_LIVE=false
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Optional: GTag
NEXT_PUBLIC_GA_ID=

# pgAdmin (for local DB inspection)
# Change these locally in .env.docker; do NOT commit secrets.
PGADMIN_DEFAULT_EMAIL=admin@local.dev
PGADMIN_DEFAULT_PASSWORD=admin_password
